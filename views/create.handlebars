<head>
    <title>{{title}}</title>
    <meta name=“viewport” content="width=device-width,initial-scale=1.0">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link href='https://fonts.googleapis.com/css?family=Raleway:500,600,700,800,900,400,300' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="/stylesheets/main.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.js" integrity="sha256-DZAnKJ/6XZ9si04Hgrsxu/8s717jcIzLy3oi35EouyE=" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <script type="text/javascript" src="/scripts/flash.js"></script>
    <link rel=“stylesheet” href=“https://cdnjs.cloudflare.com/ajax/libs/normalize/6.0.0/normalize.css” />
</head>

<body>
  <nav class="navbar navbar-inverse navbar-fixed-top container-fluid">
      <div class="container-fluid row">
          <div class="col-md-4">
            <ul class="nav nav-pills container">
              <li role="presentation"><a id="createLink" class="btnCreate navBtn navbar-btn" href="/create" role="button">Create Deck</a></li>
              <li role="presentation"><a id="myDecks" class="btnDecks navBtn navbar-btn" href="/decks" role="button">My Decks</a></li>
            </ul>
          </div>
          <div class="col-md-1">
          </div>
          <a class="navbar-brand col-md-2 container-fluid" href="/">
              <img alt="Flash" src="/img/flashLogo500tranny.png" class="logo center-block img-responsive">
          </a>
          <div class="col-md-3 container-fluid">
             <div class="form-inline justify-center searchForm">
               <input type="text" class="form-control navbar-btn quickSearch" placeholder="Quick Search" name="search">
               <span class="glyphicon glyphicon-search navbar-btn searchThing" role="button"></span>
            </div>
          </div>
          <div class="col-md-2">
              <ul class="nav nav-pills">
                  <li role="presentation"><a href="/" class="btnLogOut navBtn navbar-btn" role="button">Logout</a></li>
              </ul>
          </div>
      </div>
  </nav>
    <main class="section_overlay">
        <section class="container">
            <article class="row" id="deckRow">
                <h1 class="createHeader col-sm-3 col-md-3">Create Deck</h1>
                <form method="post">
                    <div class="row">
                        <div class="col-sm-3 col-md-3 topRow">
                            <div class="input-field">
                                <input type="text" class="form-control" id="deck_title" name="title" placeholder="Title">
                            </div>
                        </div>
                        <div class="col-sm-3 col-md-3 topRow">
                            <div class="btn btn-default upBtn addMore">+ Card</div>
                            <button class="btn btn-default upBtn createBtn" type="submit">Create</button>
                        </div>
                    </div>
                    <div class="jumbotron col-sm-3 col-md-3 createC">
                        <textarea class="form-control createText f" rows="3" name="front" data-id="1" placeholder="front"></textarea>
                        <textarea class="form-control createText" rows="7" name="back" data-id="1" placeholder="back"></textarea>
                    </div>
                    <div class="jumbotron col-sm-3 col-md-3 createC">
                        <textarea class="form-control createText f" rows="3" name="front" data-id="1" placeholder="front"></textarea>
                        <textarea class="form-control createText" rows="7" name="back" data-id="1" placeholder="back"></textarea>
                    </div>
                    <div class="jumbotron col-sm-3 col-md-3 createC">
                        <textarea class="form-control createText f" rows="3" name="front" data-id="1" placeholder="front"></textarea>
                        <textarea class="form-control createText" rows="7" name="back" data-id="1" placeholder="back"></textarea>
                    </div>
                    <div class="addMoreRows">
                    </div>
                </form>
            </article>
        </section>
    </main>
</body>
